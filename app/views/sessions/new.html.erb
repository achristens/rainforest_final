<main class="login">
  <h1>Login</h1>

  <%= form_for :session, url: sessions_path do |f|  %>
    <div class="field">
      <%#= f.label :email %>
      <%= f.email_field :email, placeholder: "Email" %>
    </div>

    <div class="field">
      <%#= f.label :password %>
      <%= f.password_field :password, placeholder: "Password" %>

    </div>

    <div class="submit">
      <%= f.submit 'Sign In' %>
    </div>
  <% end %>
</main>
